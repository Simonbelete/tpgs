{
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none",
    "dart.flutterSdkPaths": [
        "./nea"
    ],
    "cSpell.words": [
        "avaliable"
    ],
    "files.watcherExclude": {
        "**/.dart_tool": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/ilri_ges/**": true,
        "**/history/**": true,
        "**/dummy_data/**": true,
        "**/feed/**": true,
        "**/ne/**": true,
    }
}